<?xml version="1.0" encoding="UTF-8"?>

<experiment name="sample" numRuns="10">
	
	<setting key="command" value="/bin/echo" />
	
	<input source="~/Experiments/input/test.in" destination="test.in" />
	
	<abbrev param="alpha" abbrev="a" />
	<abbrev param="beta" abbrev="b" />
	<abbrev param="gamma" abbrev="g" />
	<abbrev param="nu" abbrev="n" />
	
	<value param="alpha" value="0.5" />
	
	<list param="beta">
		<item value="0.1" />
		<item value="0.3" />
		<item value="0.9" />
	</list>
	
	<range param="gamma" start="0.1" end="0.5" increment="0.1" />
	
	<range param="nu" start="0.3" end="0.6" increment="0.1" />

</experiment>
